{
  "name": "react_js",
  "version": "1.0.0",
  "description": "A React JS starter code",
  "sideEffects": "false",
  "scripts": {
    "serve": "node server.js",
    "start": "cross-env NODE_ENV=development webpack serve --env env=dev federation",
    "start-bundleanalyzer": "cross-env NODE_ENV=development webpack serve --env env=dev federation bundleAnalyzer",
    "rm-build": "rimraf build --glob",
    "rm-node_modules": "rimraf node_modules --glob",
    "rm-package-lock": "rimraf package-lock.json",
    "clean": "npm run rm-package-lock && npm run rm-node_modules",
    "build-w-rm:dev": "cross-env NODE_ENV=development webpack --env env=dev federation",
    "build:dev": "npm run rm-build && npm run build-w-rm:dev",
    "build-bundleanalyzer-w-rm:dev": "cross-env NODE_ENV=development webpack --env env=dev federation bundleAnalyzer",
    "build-bundleanalyzer:dev": "npm run rm-build && npm run build-bundleanalyzer-w-rm:dev",
    "build-w-rm": "cross-env NODE_ENV=staging webpack --env env=prod federation",
    "build": "npm run rm-build && npm run build-w-rm",
    "build-bundleanalyzer-w-rm": "cross-env NODE_ENV=staging webpack --env env=prod federation bundleAnalyzer",
    "build-bundleanalyzer": "npm run rm-build && npm run build-bundleanalyzer-w-rm",
    "build-w-rm:beta": "cross-env NODE_ENV=beta webpack --env env=prod release federation",
    "build:beta": "npm run rm-build && npm run build-w-rm:beta",
    "build-bundleanalyzer-w-rm:beta": "cross-env NODE_ENV=beta webpack --env env=prod release federation bundleAnalyzer",
    "build-bundleanalyzer:beta": "npm run rm-build && npm run build-bundleanalyzer-w-rm:beta",
    "build-w-rm:prod": "cross-env NODE_ENV=production webpack --env env=prod release no-console federation",
    "build:prod": "npm run rm-build && npm run build-w-rm:prod",
    "build-bundleanalyzer-w-rm:prod": "cross-env NODE_ENV=production webpack --env env=prod release no-console federation bundleAnalyzer",
    "build-bundleanalyzer:prod": "npm run rm-build && npm run build-bundleanalyzer-w-rm:prod",
    "build-w-rm:prod-stg": "cross-env NODE_ENV=staging webpack --env env=prod release no-console federation",
    "build:prod-stg": "npm run rm-build && npm run build-w-rm:prod-stg",
    "build-bundleanalyzer-w-rm:prod-stg": "cross-env NODE_ENV=staging webpack --env env=prod release no-console federation bundleAnalyzer",
    "build-bundleanalyzer:prod-stg": "npm run rm-build && npm run build-bundleanalyzer-w-rm:prod-stg",
    "lint-js": "eslint \"{src,public,build_utils,.}/**/*.{js,ts,jsx}\" --quiet --ignore-path .eslintignore",
    "lint-css": "stylelint \"{src,public,build_utils,.}/**/*.{css,scss}\"",
    "check-prettier": "prettier --check **/*.{scss,css,html,js}",
    "lint": "npm run lint-js && npm run lint-css && npm run check-prettier",
    "fix-js": "npm run lint-js -- --fix",
    "fix-css": "npm run lint-css -- --fix",
    "fix-prettier": "prettier --write --ignore-unknown .",
    "fix": "npm run fix-js && npm run fix-css && npm run fix-prettier",
    "lint-staged": "npx lint-staged",
    "grant-permissions:husky": "chmod +x .husky/*",
    "prepare": "husky install && npm run grant-permissions:husky",
    "commit": "git-cz",
    "test": "jest --coverage --silent",
    "test:watch": "jest --watch",
    "test:ss-update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:coverage:pre-commit": "jest --coverage --silent",
    "test:watch:quick": "jest --watch --runInBand",
    "test:coverage:quick": "jest --coverage --maxWorkers=50%",
    "test:coverage:pre-commit:quick": "jest --coverage --maxWorkers=50% --silent",
    "test:clearCache": "jest --clearCache",
    "e2e-open": "cypress open --spec cypress/e2e/app/App.cy.js",
    "e2e-run": "cypress run --spec cypress/e2e/app/App.cy.js",
    "e2e-open-all": "cypress open",
    "e2e-run-all": "cypress run",
    "e2e-open:pre-commit": "npm run e2e-open",
    "e2e-run:pre-commit": "npm run e2e-run",
    "knip": "knip",
    "knip:debug": "knip --debug",
    "knip:check-files": "knip --include files",
    "knip:w/o-devDependencies": "knip --exclude devDependencies",
    "knip-prod": "knip --production",
    "knip-prod:check-files": "knip --production --include files",
    "knip:pre-commit": "npm run knip-prod:check-files"
  },
  "author": "Arpit Malik",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/eslint-parser": "^7.24.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "chalk": "^4.1.2",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "cypress": "^13.8.1",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-cypress": "^3.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "express": "^4.19.2",
    "express-static-gzip": "^2.1.7",
    "globals": "^15.1.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^9.2.20",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.13.0",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.11",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "sass": "^1.77.0",
    "sass-loader": "^14.2.1",
    "sass-resources-loader": "^2.2.5",
    "stylelint": "^14.16.1",
    "stylelint-config-clean-order": "^5.4.2",
    "stylelint-config-hudochenkov": "^8.0.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.0.0",
    "stylelint-order": "^6.0.4",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.4.5",
    "validate-branch-name": "^1.3.0",
    "web-vitals": "^3.5.2",
    "webpack": "^5.91.0",
    "webpack-assets-manifest": "^5.2.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0",
    "workbox-webpack-plugin": "^7.1.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "rxjs": "^7.8.1",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  }
}
