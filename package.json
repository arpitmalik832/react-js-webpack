{
  "name": "react-js",
  "version": "1.0.0",
  "description": "A React JS starter code",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack serve --mode development",
    "lint-js": "eslint \"{src,public,build_utils,.}/**/*.{js,ts,jsx}\" --quiet --ignore-path .eslintignore",
    "lint-css": "stylelint \"{src,public,build_utils,.}/**/*.{css,scss}\"",
    "check-prettier": "prettier --check **/*.{scss,css,html,js}",
    "lint": "npm run lint-js && npm run lint-css && npm run check-prettier",
    "fix-js": "npm run lint-js -- --fix",
    "fix-css": "npm run lint-css -- --fix",
    "fix-prettier": "prettier --write --ignore-unknown .",
    "fix": "npm run fix-js && npm run fix-css && npm run fix-prettier",
    "lint-staged": "npx lint-staged",
    "grant-permissions:husky": "chmod +x .husky/*",
    "test": "jest --coverage --silent",
    "test:watch": "jest --watch",
    "test:ss-update": "jest -u",
    "test:coverage": "jest --coverage",
    "test:coverage:pre-commit": "jest --coverage --silent",
    "test:watch:quick": "jest --watch --runInBand",
    "test:coverage:quick": "jest --coverage --maxWorkers=50%",
    "test:coverage:pre-commit:quick": "jest --coverage --maxWorkers=50% --silent",
    "test:clearCache": "jest --clearCache"
  },
  "author": "Arpit Malik",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/eslint-parser": "^7.24.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.1.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "commitiquette": "^1.2.1",
    "commitizen": "^4.3.0",
    "css-loader": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.1.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.9.0",
    "node-sass": "^9.0.0",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "sass-loader": "^14.2.1",
    "stylelint": "^14.16.1",
    "stylelint-config-clean-order": "^5.4.2",
    "stylelint-config-hudochenkov": "^8.0.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.0.0",
    "stylelint-order": "^6.0.4",
    "validate-branch-name": "^1.3.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
